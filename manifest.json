{
  "manifest_version": 2,
  "name": "Tab Saver",
  "options_ui": {
    "page": "Options.html",
    "chrome_style": false
  },
  "version": "3.0.2",
  "description": "Best way to save tabs!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "minimum_chrome_version": "45",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
  }

