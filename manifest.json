{
  "manifest_version": 2,
  "name": "Tab Saver",
  "version": "3.0.4",
  "description": "Best way to save tabs!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "minimum_chrome_version": "45",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "downloads"
  ]
  }

